    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mongoose": "^9.2.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.1",
    "joi": "^17.9.2",
    "mongoose": "^7.1.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
